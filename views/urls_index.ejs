<!DOCTYPE html>
<html lang="en">
<head>
  <% include partials/_head %>
</head>
<body>
  <header>
  <% include partials/_header %>
  </header>
  <ul>
    <% for (let prop in urls) { %>
      <% if (urls[prop].ownedByCurrentUser) {%>
        <li>
        <%= prop %> - <%= JSON.stringify(urls[prop].longURL) %>
        </li>
      <% } %>
      <form action="/urls/<%= prop %>" method="GET">
        <% if (urls[prop].ownedByCurrentUser) {%>
          <button type="submit"><a href="/urls/<%= prop %>" style="text-decoration:none">Edit</a></button>
        <% } %>
      </form>
      <form class="" action="/urls/<%= prop %>/delete" method="POST">
        <% if (urls[prop].ownedByCurrentUser) {%>
          <button type="submit">Delete</button>
        <% } %>
      </form>
  <% } %>
  </ul>
  <a href="/urls/new" style="text-decoration:none">Create a New Short Link</a>
</body>
</html>
